<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vasiliy.encyclopedia_of_the_sky" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyAppTheme" >
        <activity
            android:name=".Activitys.MainActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activitys.EncyclopediaActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity android:name=".Activitys.ConstellationListActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Activitys.ConstellationViewActivity"
            android:label="@string/title_activity_constellation_view"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>
